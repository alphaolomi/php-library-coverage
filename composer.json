{
    "name": "alphaolomi/library-docs",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alpha Olomi",
            "homepage": "https://github.com/alphaolomi"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10"
    },
    "autoload": {
        "psr-4": {
            "Alphaolomi\\Library\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alphaolomi\\Library\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
